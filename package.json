{
  "name": "vscode-gif-cards",
  "displayName": "vscode-gif-cards",
  "repository": {
    "type": "git",
    "url": "https://github.com/ericadamski/vscode-gif-cards.git"
  },
  "description": "A simple app that allows you to query for gifs from vscode",
  "version": "0.0.1",
  "publisher": "ericTurf",
  "engines": {
    "vscode": "^1.21.0"
  },
  "categories": ["Other"],
  "activationEvents": ["onCommand:vscode-card-gifs.searchGIF"],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "vscode-card-gifs.searchGIF",
        "title": "GIF"
      }
    ],
    "keybindings": [
      {
        "key": "alt+cmd+g",
        "command": "vscode-card-gifs.searchGIF"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "opn-url": "^1.0.1"
  }
}
